{
    "name": "railt/carbon-extension",
    "type": "library",
    "description": "An extension that provides objects for working with a date and time using the Carbon library",
    "license": "MIT",
    "keywords": ["carbon", "date", "time", "datetime", "extension", "graphql", "railt"],
    "homepage": "http://railt.org",
    "support": {
        "issues": "https://github.com/railt/carbon-extension/issues",
        "source": "https://github.com/railt/carbon-extension"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "railt/railt": "1.2.*|dev-master",
        "nesbot/carbon": "~1.9"
    },
    "autoload": {
        "psr-4": {
            "Railt\\CarbonExtension\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Railt\\Tests\\CarbonExtension\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
